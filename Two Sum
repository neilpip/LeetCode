class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        required = {}
        for i in range(len(nums)):
            if target - nums[i] in required:
                return [required[target - nums[i]], i]
            else:
                required[nums[i]] = i
                
inputNums = [2, 7, 11, 15]
s1 = Solution()
print(s1.twoSum(inputNums, 9))

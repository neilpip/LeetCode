class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        return Counter(s) == Counter(t)
    
        if len(s) != len(t):
            return False
        
        count_s, count_t = {}, {}
        
        for x in range(len(s)):
            count_s[s[x]] = 1 + count_s.get(s[x], 0)
            count_t[t[x]] = 1 + count_t.get(t[x], 0)
        for y in count_s:
            if count_s[y] != count_t.get(y, 0):
                return False
        
        return True
